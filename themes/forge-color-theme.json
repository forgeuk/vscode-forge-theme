{
  "name": "Forge",
  "type": "dark",
  "colors": {
    "foreground": "#F8F8F8",
    "input.background": "#65A1A810",
    "input.border": "#2D2D3F",
    "inputOption.activeBorder": "#F47A97",
    "activityBar.background": "#2D2D3F",
    "list.activeSelectionBackground": "#65A1A840",
    "list.focusBackground": "#65A1A840",
    "list.hoverBackground": "#65A1A810",
    "list.inactiveSelectionBackground": "#65A1A820",
    "sideBar.background": "#2D2D3F",
    "editor.background": "#2D2D3F",
    "editor.foreground": "#65A1A8",
    "editorLineNumber.foreground": "#616166",
    "editorLineNumber.activeForeground": "#F47A97",
    "editorCursor.background": "#F47A97",
    "editor.selectionBackground": "#65A1A840",
    "editor.hoverHighlightBackground": "#65A1A820",
    "editor.lineHighlightBorder": "#65A1A820",
    "editorLink.activeForeground": "#92D6F4",
    "statusBar.background": "#2D2D3F",
    "statusBar.debuggingBackground": "#FFC166",
    "statusBar.debuggingForeground": "#2D2D3F",
    "gitDecoration.addedResourceForeground": "#94C979",
    "gitDecoration.conflictingResourceForeground": "#EA7F6A",
    "gitDecoration.deletedResourceForeground": "#EA7F6A",
    "gitDecoration.ignoredResourceForeground": "#9F9FA6",
    "gitDecoration.modifiedResourceForeground": "#FFC166",
    "gitDecoration.untrackedResourceForeground": "#94C979"
  },
  "tokenColors": [
    // Comments, separators
    {
      "scope": "comment",
      "settings": {
        "foreground": "#9F9FA6",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "meta.separator",
      "settings": {
        "foreground": "#9F9FA6"
      }
    },

    // Constants, HTML tags, 'this', headings
    {
      "scope": "constant",
      "settings": {
        "foreground": "#EA7F6A"
      }
    },
    {
      "scope": "support.constant",
      "settings": {
        "foreground": "#EA7F6A"
      }
    },
    // {
    //   "scope": "variable.other.constant",
    //   "settings": {
    //     "foreground": "#EA7F6A"
    //   }
    // },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#EA7F6A"
      }
    },
    {
      "scope": "variable.language.this",
      "settings": {
        "foreground": "#EA7F6A"
      }
    },
    {
      "scope": "markup.heading",
      "settings": {
        "foreground": "#EA7F6A"
      }
    },
    {
      "scope": "entity.name.section.group-title",
      "settings": {
        "foreground": "#EA7F6A"
      }
    },

    // Strings, bold/italic text
    {
      "scope": "string",
      "settings": {
        "foreground": "#94C979"
      }
    },
    {
      "scope": "markup.inline.raw.string",
      "settings": {
        "foreground": "#94C979"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "foreground": "#94C979"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "foreground": "#94C979",
        "fontStyle": "italic"
      }
    },

    // Variables, HTML attributes, list points
    {
      "scope": "variable",
      "settings": {
        "foreground": "#F47A97"
      }
    },
    {
      "scope": "support.variable",
      "settings": {
        "foreground": "#F47A97"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#F47A97",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "punctuation.definition.list",
      "settings": {
        "foreground": "#F47A97"
      }
    },
    {
      "scope": "keyword.other.definition",
      "settings": {
        "foreground": "#F47A97"
      }
    },
    {
      "scope": "meta.object-literal.key",
      "settings": {
        "foreground": "#F47A97"
      }
    },

    // Functions, links
    {
      "scope": "support.function",
      "settings": {
        "foreground": "#92D6F4"
      }
    },
    {
      "scope": "markup.underline.link",
      "settings": {
        "foreground": "#92D6F4"
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#92D6F4"
      }
    },

    // Classes, Types
    {
      "scope": "support.class",
      "settings": {
        "foreground": "#FFC166"
      }
    },
    {
      "scope": "entity.name.type",
      "settings": {
        "foreground": "#FFC166"
      }
    },
    {
      "scope": "entity.other.inherited-class",
      "settings": {
        "foreground": "#FFC166"
      }
    },
    {
      "scope": "support.type.primitive",
      "settings": {
        "foreground": "#FFC166"
      }
    }
  ]
}
