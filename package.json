{
  "name": "vscode-forge-theme",
  "displayName": "Forge Theme",
  "description": "A dark theme using Forge's unique and stylish branding",
  "version": "1.1.0",
  "publisher": "forge",
  "engines": {
    "vscode": "^1.14.0"
  },
  "categories": ["Themes"],
  "icon": "icon.jpg",
  "contributes": {
    "themes": [
      {
        "label": "Forge",
        "uiTheme": "vs-dark",
        "path": "themes/forge-color-theme.json"
      }
    ],
    "markdown.previewStyles": ["./styles/markdown.css"]
  },
  "galleryBanner": {
    "color": "#2d2d3f",
    "theme": "dark"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/forgeuk/vscode-forge-theme"
  }
}
